{
  "name": "immutable-arrays",
  "version": "3.0.1",
  "description": "Immutable versions of normally mutable array methods",
  "main": "dist/immutable-arrays.cjs.js",
  "module": "dist/immutable-arrays.esm.js",
  "unpkg": "dist/immutable-arrays.umd.min.js",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --colors ./test/*.spec.js",
    "coverage": "nyc --reporter=text --reporter=lcov npm test",
    "clean": "rimraf dist coverage",
    "prepare": "npm run clean && npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/georapbox/immutable-arrays.git"
  },
  "bugs": {
    "url": "https://github.com/georapbox/immutable-arrays/issues"
  },
  "homepage": "https://github.com/georapbox/immutable-arrays#readme",
  "keywords": [
    "immutable",
    "array",
    "methods",
    "push",
    "pop",
    "delete",
    "unshift",
    "shift",
    "splice",
    "reverse"
  ],
  "author": "George Raptis <georapbox@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "~7.4.3",
    "@babel/preset-env": "~7.4.3",
    "@babel/register": "~7.4.0",
    "chai": "~4.2.0",
    "cross-env": "~5.2.0",
    "eslint": "~5.16.0",
    "mocha": "~6.1.0",
    "nyc": "~13.3.0",
    "rimraf": "~2.6.3",
    "rollup": "~1.9.0",
    "rollup-plugin-babel": "~4.3.2",
    "rollup-plugin-terser": "~4.0.4",
    "sinon": "~7.3.1",
    "sinon-chai": "~3.3.0"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      ".babelrc.js",
      "rollup.config.js",
      "dist"
    ]
  }
}
