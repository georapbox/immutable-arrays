{
  "name": "immutable-arrays",
  "version": "0.4.2",
  "description": "Immutable versions of normally mutable array methods",
  "main": "lib/immutableArrays.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "coverage": "nyc --reporter=html --reporter=text npm test",
    "build": "webpack --env=build",
    "dev": "webpack --env=dev",
    "watch": "webpack --progress --colors --watch --env=dev",
    "lint": "eslint src/**/*.js",
    "prepublish": "npm run dev & npm run build",
    "docs": "jsdoc2md 'src/immutable-delete.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/georapbox/immutable-arrays.git"
  },
  "bugs": {
    "url": "https://github.com/georapbox/immutable-arrays/issues"
  },
  "homepage": "https://github.com/georapbox/immutable-arrays#readme",
  "keywords": [
    "immutable",
    "array",
    "methods",
    "push",
    "pop",
    "delete",
    "unshift",
    "shift",
    "splice",
    "reverse"
  ],
  "author": "George Raptis <georapbox@gmail.com> (georapbox.github.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-core": "~6.25.0",
    "babel-loader": "~7.1.0",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-preset-es2015": "~6.24.1",
    "chai": "~4.0.2",
    "eslint": "~4.1.0",
    "eslint-loader": "~1.8.0",
    "mocha": "~3.4.2",
    "nyc": "~11.0.2",
    "webpack": "~3.0.0",
    "yargs": "~8.0.2"
  }
}
