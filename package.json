{
  "name": "immutable-arrays",
  "version": "2.0.1",
  "description": "Immutable versions of normally mutable array methods",
  "main": "dist/commonjs/immutable-arrays.js",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --colors ./test/*.spec.js",
    "coverage": "nyc --reporter=text --reporter=lcov npm test",
    "lint": "eslint src/**/*.js",
    "build:umdmin": "webpack --mode=production --output-library-target=umd",
    "build:umd": "webpack --mode=development --output-library-target=umd",
    "build:commonjs": "webpack --mode=development --output-library-target=commonjs2",
    "build:commonjs-fn": "webpack --mode=development --output-library-target=commonjs2 --config=webpack.config.commonjs.standalone.js",
    "build": "npm-run-all build:*",
    "dev:umd": "webpack --progress --colors --watch --mode=development --output-library-target=umd",
    "dev:commonjs": "webpack --progress --colors --watch --mode=development --output-library-target=commonjs2",
    "dev:commonjs-fn": "webpack --progress --colors --watch --mode=development --output-library-target=commonjs2 --config=webpack.config.commonjs.standalone.js",
    "dev": "npm-run-all --parallel dev:*",
    "prepublishOnly": "npm-run-all test build",
    "prepack": "npm-run-all test build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/georapbox/immutable-arrays.git"
  },
  "bugs": {
    "url": "https://github.com/georapbox/immutable-arrays/issues"
  },
  "homepage": "https://github.com/georapbox/immutable-arrays#readme",
  "keywords": [
    "immutable",
    "array",
    "methods",
    "push",
    "pop",
    "delete",
    "unshift",
    "shift",
    "splice",
    "reverse"
  ],
  "author": "George Raptis <georapbox@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.1.2",
    "@babel/preset-env": "~7.1.0",
    "@babel/register": "~7.0.0",
    "babel-loader": "~8.0.4",
    "chai": "~4.2.0",
    "cross-env": "~5.2.0",
    "eslint": "~5.7.0",
    "eslint-loader": "~2.1.1",
    "mocha": "~5.2.0",
    "npm-run-all": "~4.1.5",
    "nyc": "~13.1.0",
    "webpack": "~4.26.1",
    "webpack-cli": "~3.1.2",
    "yargs": "~12.0.2"
  }
}
