{
  "name": "immutable-arrays",
  "version": "4.0.0",
  "description": "Immutable versions of normally mutable array methods",
  "main": "dist/immutable-arrays.cjs.js",
  "module": "dist/immutable-arrays.esm.js",
  "unpkg": "dist/immutable-arrays.umd.min.js",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --colors ./test/*.spec.js",
    "coverage": "nyc --reporter=text --reporter=lcov npm test",
    "clean": "rimraf dist coverage",
    "prepare": "npm-run-all clean lint test build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/georapbox/immutable-arrays.git"
  },
  "bugs": {
    "url": "https://github.com/georapbox/immutable-arrays/issues"
  },
  "homepage": "https://github.com/georapbox/immutable-arrays#readme",
  "keywords": [
    "immutable",
    "array",
    "methods",
    "push",
    "pop",
    "delete",
    "unshift",
    "shift",
    "splice",
    "reverse"
  ],
  "author": "George Raptis <georapbox@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "~7.6.2",
    "@babel/preset-env": "~7.6.3",
    "@babel/register": "~7.6.2",
    "chai": "~4.2.0",
    "cross-env": "~6.0.3",
    "eslint": "~6.5.1",
    "mocha": "~6.2.1",
    "npm-run-all": "~4.1.5",
    "nyc": "~14.1.1",
    "rimraf": "~3.0.0",
    "rollup": "~1.24.0",
    "rollup-plugin-babel": "~4.3.3",
    "rollup-plugin-terser": "~5.1.2",
    "sinon": "~7.5.0",
    "sinon-chai": "~3.3.0"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      ".babelrc.js",
      "rollup.config.js",
      "dist"
    ]
  }
}
